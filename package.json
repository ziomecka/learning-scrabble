  "description": "scrabble",
  "main": "index.js",
  "scripts": {
    "bundleAll": "webpack --config webpack.bundle.babel.js && webpack --config webpack.server.babel.js",
    "bundleApp": "webpack --config webpack.bundle.babel.js",
    "bundleServer": "webpack --config webpack.bundle.babel.js",
    "watchApp": "webpack --watch --progress --config webpack.bundle.babel.js",
    "watchServer": "webpack --watch --progress --config webpack.server.babel.js",
    "preserver": "npm run bundle",
    "server": "webpack-dev-server --config webpack.dev.babel.js",
    "preanalyse": "webpack --config webpack.bundle.babel.js",
    "analyse": "webpack --config webpack.analyse.babel.js",
    "test": "babel-node ./node_modules/karma/bin/karma start karma.conf.babel.js",
    "test-chrome": "babel-node ./node_modules/karma/bin/karma start karma.conf.babel.js --browsers Chrome",
    "coverage": "babel-node ./node_modules/karma/bin/karma start karma.conf.coverage.babel.js",
    "coverage-chrome": "babel-node ./node_modules/karma/bin/karma start karma.conf.coverage.babel.js --browsers Chrome",
    "prestart": "npm run bundle",
    "startApp": "babel-node ./app/index.js --presets es2015",
    "prestartBundle": "npm run bundle",
    "startBundle": "babel-node ./bundle/app.bundle.js --presets es2015",
    "nodeInspectBundle": "SET NODE_ENV=development && SET DEBUG=express:* && babel-node --inspect-brk ./bundle/server/server.js --presets es2015",
    "nodemon": "SET NODE_ENV=development && nodemon bundle/server/server.js --exec babel-node",
    "start": "SET DEBUG=* && node ./bundle/server.js",
    "redis": "rdcli -h a-redis-address -p 22839 -a p2e369183f7f2da12e70035f77868e324f1c02537b5288e906b05b6213b0b8471"
